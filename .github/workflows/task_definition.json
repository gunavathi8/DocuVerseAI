{
  "family": "DocuVerseAI",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::647538623542:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "docu-verse-ai-container",
      "image": "647538623542.dkr.ecr.eu-north-1.amazonaws.com/docuverseai",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "docu-verse-ai-container-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "ENV",
          "value": "production"
        }
      ],
      "secrets": [
        {
          "name": "API_KEYS",
          "valueFrom": "arn:aws:secretsmanager:eu-north-1:647538623542:secret:api_keys-Ul81Ak"
        }
        
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/DocuVerseAI",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}